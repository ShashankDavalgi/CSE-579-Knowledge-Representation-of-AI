%%%%%%%%%%% Set shelf location at 3*3 for pickup %%%%%%%%%%%
init(object(shelf,2),value(at,pair(3,3))).

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%%%%%%%% PICKUP %%%%%%%%%%%
occurs(object(robot,1),pickup,0).

